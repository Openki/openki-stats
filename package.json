  "scripts": {
    "dev": "npm run extract-translations && npm run type-check && meteor --inspect --settings settings/dev.json",
    "test": "npm run type-check && MOCHA_GREP=$npm_config_grep TEST_BROWSER_DRIVER=chrome TEST_CHROME_ARGS=--user-data-dir=$(mktemp -u -p $HOME -t tmp4meteor_tests.XXXXXXXXXX) meteor test --once --driver-package meteortesting:mocha --inspect --settings settings/ci.json",
    "app-test": "npm run type-check && MOCHA_GREP=$npm_config_grep TEST_BROWSER_DRIVER=chrome TEST_CHROME_ARGS=--user-data-dir=$(mktemp -u -p $HOME -t tmp4meteor_tests.XXXXXXXXXX) meteor test --full-app --once --driver-package meteortesting:mocha --inspect --settings settings/ci.json",
    "test-live": "npm run type-check && MOCHA_GREP=$npm_config_grep meteor test --full-app --driver-package meteortesting:mocha --inspect --settings settings/ci.json",
    "type-check": "npx tsc --checkJs false",
    "es-lint": "eslint . --ext .js,.ts",
    "sass-lint": "stylelint '**/*.scss'",
    "html-format": "html-beautify '{client,imports,private,public,server}/**/*.html' -r",
    "extract-translations": "rm imports/startup/both/i18n/en.json -f && npx i18next",
    "reset": "meteor reset"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/Openki/Openki.git"
  },
