    "@types/sanitize-html": "2.3.2",
    "@types/seedrandom": "2.4.28",
    "@typescript-eslint/eslint-plugin": "4.29.2",
    "@typescript-eslint/parser": "4.29.2",
    "autoprefixer": "10.3.1",
    "assertion-error": "1.1.0",
    "chai": "4.3.4",
    "chromedriver": "90.0.1",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base-typescript-prettier": "4.1.0",
