    "stylelint-config-sass-guidelines": "8.0.0",
    "stylelint-prettier": "1.2.0",
    "typescript": "4.2.2"
  },
  "scripts": {
    "dev": "npx tsc --checkJs false && meteor --inspect --settings settings/dev.json",
    "test": "npx tsc --checkJs false && MOCHA_GREP=$npm_config_grep TEST_BROWSER_DRIVER=chrome TEST_CHROME_ARGS=--user-data-dir=$(mktemp -u -p $HOME -t tmp4meteor_tests.XXXXXXXXXX) meteor test --once --driver-package meteortesting:mocha --inspect --settings settings/ci.json",
    "app-test": "npx tsc --checkJs false && MOCHA_GREP=$npm_config_grep TEST_BROWSER_DRIVER=chrome TEST_CHROME_ARGS=--user-data-dir=$(mktemp -u -p $HOME -t tmp4meteor_tests.XXXXXXXXXX) meteor test --full-app --once --driver-package meteortesting:mocha --inspect --settings settings/ci.json",
    "test-live": "npx tsc --checkJs false && MOCHA_GREP=$npm_config_grep meteor test --full-app --driver-package meteortesting:mocha --inspect --settings settings/ci.json",
    "type-check": "npx tsc --checkJs false",
    "es-lint": "eslint . --ext .js,.ts",
    "sass-lint": "stylelint '**/*.scss'",
    "html-format": "html-beautify '{client,imports,private,public,server}/**/*.html' -r",
    "reset": "meteor reset && rm server/extracts.msgfmt~"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/Openki/Openki.git"
  },
  "keywords": [
    "education",
    "self-organization",
    "calendar",
    "community"
  ],
  "author": "Openki-devs",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://gitlab.com/Openki/Openki/issues"
  },
  "homepage": "https://about.openki.net"
}
