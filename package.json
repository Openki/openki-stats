    "@popperjs/core": "2.9.2",
    "postcss": "8.2.15",
    "postcss-load-config": "3.0.1",
    "sanitize-html": "2.4.0",
    "seedrandom": "3.0.5",
    "shariff": "3.2.1",
    "url": "0.11.0"
  },
  "devDependencies": {
    "@types/bootstrap": "5.1.2",
    "@types/bootstrap-datepicker": "0.0.14",
    "@types/chai": "4.2.15",
